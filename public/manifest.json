{
  "manifest_version":3,
  "name": "메뉴 크롬확장 프로그램-nextjs",
  "version":"0.0.2",
  "description": "메뉴 크롬확장 프로그램-nextjs",
  "background": {
    "service_worker": "background.js"
  },  
  "action":{
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
